{
  "presets": ["@babel/preset-react", "@babel/preset-env"],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "babel-plugin-react-css-modules",
          {
            "generateScopedName": "[local]"
          }
        ]
      ]
    }
  }
}
