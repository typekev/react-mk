{
  "presets": ["@babel/preset-react", "@babel/preset-env"],
  "plugins": [
    ["styled-jsx/babel", { "optimizeForSpeed": true }],
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}
